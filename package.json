{
  "name": "poolcontroller",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:server": "./node_modules/.bin/webpack --config ./webpack.server.config.js",
    "build:client": "./node_modules/.bin/webpack --config ./webpack.client.config.js",
    "build:dev": "./node_modules/.bin/webpack --env.development --config ./webpack.server.config.js",
    "start": "node ./Server.bundle.js",
    "debug": "node --nolazy --debug=5858 Server.bundle.js",
    "dev": "npm run build:dev && npm run debug",
    "remote-debug": "node --debug=5858 ./Server.js",
    "test": "npm run lint && npm run flow && npm run tests:execute",
    "tests:execute": "mocha test --compilers js:babel-core/register --recursive",
    "tests:watch": "mocha test --compilers js:babel-core/register --recursive --watch",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov mocha --recursive",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "@types/body-parser": "^1.16.3",
    "@types/cookie-parser": "^1.3.30",
    "@types/debug": "0.0.29",
    "@types/express": "^4.0.35",
    "@types/mongodb": "^2.1.43",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.12",
    "@types/react-redux": "^4.4.38",
    "@types/react-router": "^4.0.4",
    "@types/redux": "^3.6.0",
    "@types/socket.io": "^1.4.29",
    "@types/winston": "^2.3.0",
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "dateformat": "^2.0.0",
    "debug": "^2.6.1",
    "ejs": "~2.4.1",
    "events": "^1.1.1",
    "express": "~4.13.4",
    "history": "^1.17.0",
    "mongodb": "^2.2.22",
    "morgan": "~1.7.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.5.0",
    "util": "^0.10.3",
    "winston": "^2.3.1"
  },
  "optionalDependencies": {
    "i2c-bus": "^1.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.3",
    "awesome-typescript-loader": "^3.1.2",
    "chai": "^3.5.0",
    "coveralls": "^2.11.16",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "nyc": "^10.1.2",
    "postcss-loader": "^1.3.0",
    "precss": "^1.4.0",
    "sass-loader": "^6.0.0",
    "sinon": "^1.17.7",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.13.1",
    "typescript": "^2.2.2",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
  }
}
