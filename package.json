  "private": true,
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:server": "./node_modules/.bin/webpack --config ./webpack.server.config.js",
    "build:client": "./node_modules/.bin/webpack --config ./webpack.client.config.js",
    "build:dev": "./node_modules/.bin/webpack --env.development --config ./webpack.server.config.js",
    "start": "node ./Server.bundle.js",
    "debug": "node --nolazy --debug=5858 Server.bundle.js",
    "dev": "npm run build:dev && npm run debug",
    "remote-debug": "node --debug=5858 ./Server.js",
    "test": "mocha test --compilers ts:ts-node/register --recursive",
    "tests:watch": "mocha test --compilers js:babel-core/register --recursive --watch",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov mocha --recursive",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
